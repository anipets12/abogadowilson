{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [],
  "routes": [
    {
      "src": "/api/config",
      "dest": "/api/config"
    },
    {
      "src": "/api/config/(.*)",
      "dest": "/api/config"
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy"
    },
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy?url=$1"
    },
    {
      "src": "/(.*)\\.(.*)$",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
