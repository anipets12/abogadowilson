{
  "version": 1,
  "include": [
    "/*"
  ],
  "exclude": [],
  "routes": [
    {
      "src": "/favicon.ico",
      "dest": "/favicon.ico"
    },
    {
      "src": "/favicon.svg",
      "dest": "/favicon.svg"
    },
    {
      "src": "/manifest.json",
      "dest": "/manifest.json"
    },
    {
      "src": "/api/config",
      "dest": "/api/config"
    },
    {
      "src": "/api/config/(.*)",
      "dest": "/api/config"
    },
    {
      "src": "/api/proxy",
      "dest": "/api/proxy"
    },
    {
      "src": "/api/proxy/(.*)",
      "dest": "/api/proxy?url=$1"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)\\.(.*)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
